.container {
    position: relative;
    width: 100%;
    max-width: 640px;
    min-height: 100%;
    margin: 0 auto;
    padding: 0 0 16px;
    overflow-x: hidden;
    background-color: #eceded;

    overflow-y: hidden;
}

.my {
    height: 100px;
    position: relative;
    width: 100%;
    z-index: 1;
    background-color: #fff;
}

.my_head {
