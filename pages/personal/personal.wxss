    padding: 7.5px 0 1.5px;
    
}

.my_head_img {
    border-radius: 50%;
    -webkit-border-radius: 50%;
    box-shadow: 0 0 0 2px #fff;
    height: 65px;
    width: 65px;
    display: inline-block;
    background-repeat: no-repeat;
}

.h4 {
    
    font-weight: bold;
}

.nav {
    background: none repeat scroll 0 0 ;
    position: relative;
}

.nav {
    padding-right: 0;
}

.nav:before {
    border-bottom: 1px solid #d3d3d3;
    content: "";
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
}

.nav_item {
    padding-left:  10px;
    position: relative;
    display: flex
}

.a {
    text-decoration: none;
    color: #333;
}

.a {
    background-color: transparent;
    text-decoration: none;
    -webkit-tap-highlight-color: transparent;
}

.nav_item_hd {
    color: #000;
    font-size: 14px;
    height: 45px;
    line-height: 45px;
    padding-left: 10px;
    position: relative;
    z-index: 1;

    border-bottom: 1px solid #d3d3d3;

}

.nav_item_hd_last {
    color: #000;
    font-size: 14px;
    height: 45px;
    line-height: 45px;
    padding-left: 10px;
    position: relative;
    z-index: 1;

    

}

